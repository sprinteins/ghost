  "scripts": {
    "build": "npm run lint && npm run frontend-build && npm run backend-build&& npm run test-build",
    "frontend-dev": "node scripts/start.js",
    "frontend-build": "node scripts/build.js",
    "frontend-test": "node scripts/test.js --config jest.config.js",
    "start": "concurrently \"yarn frontend-dev\" \"yarn backend-dev\"",
    "start-win": "concurrently \"yarn frontend-dev\" \"yarn backend-dev-win\"",
    "test": "NODE_ENV=test&& npm run build && npm run test-backend&& node scripts/test.js",
    "test-build": "npm run lint &&tsc --build tsconfig.test.json",
    "test-win": "SET NODE_ENV=test&& npm run build && npm run test-backend&& node scripts/test.js",
    "backend-dev": "npm run backend-build && ELECTRON_START_URL=http://localhost:3000 electron ./build/electron.js",
    "backend-dev-win": "npm run backend-build && SET ELECTRON_START_URL=http://localhost:3000 && electron ./build/electron.js",
    "backend-build": "npm run lint && tsc --build tsconfig.electron.json",
    "pack": "electron-builder",
    "lint": "tslint 'backend/**/*.{ts,tsx}' 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "test-backend": "mocha"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "keywords": [
    "git",
    "analysis",
    "code"
  ],
  "license": "MIT",
  "jest": {
    "verbose": true
  },
  "build": {
    "files": [
      "build/**/**/*",
      "node_modules/**/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
